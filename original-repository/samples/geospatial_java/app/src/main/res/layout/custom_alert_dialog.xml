<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingTop="10dp"
    android:paddingBottom="10dp" >

    <TextView
        android:id="@+id/privacy_notice_dialog_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/privacy_notice_dialog_title"
        android:textSize="20sp"
        android:textStyle="bold"
        android:textColor="#000000"
        android:layout_marginBottom="20dp" />

    <TextView
        android:id="@+id/privacy_notice_play_services_for_ar_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/privacy_notice_play_services_for_ar_title"
        android:textSize="14sp"
        android:textStyle="bold"
        android:textColor="#000000"
        android:layout_marginBottom="10dp" />
    <TextView
        android:id="@+id/privacy_notice_play_services_for_ar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/privacy_notice_play_services_for_ar"
        android:layout_marginBottom="20dp" />

    <TextView
        android:id="@+id/privacy_notice_anchors_geospatial_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/privacy_notice_anchors_geospatial_title"
        android:textSize="14sp"
        android:textStyle="bold"
        android:textColor="#000000"
        android:layout_marginBottom="10dp" />
    <TextView
        android:id="@+id/privacy_notice_anchors_geospatial"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/privacy_notice_anchors_geospatial" />

</LinearLayout>
